{
	"name": "alfredoramos/imgur",
	"type": "phpbb-extension",
	"description": "Imgur extension for phpBB",
	"homepage": "https://github.com/AlfredoRamos/phpbb-ext-imgur",
	"version": "1.7.0-dev",
	"time": "2020-07-10",
	"keywords": ["phpbb", "extension", "imgur", "uploader"],
	"license": "GPL-2.0-only",
	"authors": [
		{
			"name": "Alfredo Ramos",
			"email": "alfredo.ramos@yandex.com",
			"homepage": "https://alfredoramos.mx/",
			"role": "Lead Developer"
		}
	],
	"require": {
		"php": "^7.1.3",
		"composer/installers": "^1.6.0",
		"j0k3r/php-imgur-api-client": "3.x-dev"
	},
	"require-dev": {
		"phing/phing": "^2.16.0",
		"pear/archive_tar": "^1.4.0"
	},
	"extra": {
		"display-name": "Imgur",
		"soft-require": {
			"phpbb/phpbb": "~3.3.0"
		},
		"version-check": {
			"host": "alfredoramos.mx",
			"directory": "/phpbb-extensions/versions",
			"filename": "imgur.json",
			"ssl": true
		}
	}
}
